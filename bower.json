{
  "name": "frontend-monster",
  "description": "A tool for building website UI toolkits",
  "main": "README.md",
  "authors": [
    "DevGroup.ru"
  ],
  "license": "MIT",
  "keywords": [
    "toolkit",
    "styleguide",
    "framework",
    "scaffold",
