    "smooth-scroll": "https://github.com/cferdinandi/smooth-scroll.git#^9.1.3",
    "jqueryui-touch-punch": "*",
    "chosen": "^1.5.1",
    "is_js": "^0.7.6",
    "malihu-custom-scrollbar-plugin": "^3.1.3",
    "jquery-popup-overlay": "^1.6.0",
    "owl.carousel": "^2.0.0-beta.3"
